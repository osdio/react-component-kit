{
  "name": "react-listview",
  "version": "1.0.0",
  "description": "react listview",
  "scripts": {
    "test": "eslint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "NODE_ENV=production webpack -p",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "start": "NODE_ENV=development webpack-dev-server --inline --hot --port 8088 --colors",
    "postinstall": "bower install"
  },
  "author": "",
  "license": "BSD",
  "keywords": [
    "react",
    "reactjs",
    "react-component"
  ],
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "css-loader": "^0.23.0",
    "es5-shim": "^4.4.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "html-webpack-plugin": "^1.7.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.9",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
